var x = 4;
var y = 1;

if (x == y || y > x) {

	console.log('Tentativa 1');
}
else if (x < 3) {

	console.log('Tentativa 2');
}
else{

	console.log('Tentativa 3')
}

// Switch case codição logica 2 

var nome = 'Gustavo Henrique';

switch(nome){

	case 'Gustavo':
		console.log('Gustavo');
		break;

	case 'Gustavo Henrique':
		console.log('OK seja bem-vindo');
		break;

	default:
		console.log('Não achou sua seleção');
}